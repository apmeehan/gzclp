{
  "expo": {
    "name": "GZCLP",
    "description": "An app that makes following the GZCLP beginner strength-training program easy.",
    "slug": "gzclp",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
			"bundleIdentifier": "com.petermeehan.gzclp"
		},
		"android": {
			"package": "com.petermeehan.gzclp"
		}
  }
}
